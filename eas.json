{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EAS_BUILD_PROFILE": "development"
      },
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EAS_BUILD_PROFILE": "development"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      }
    },
    "dev-testflight": {
      "distribution": "store",
      "env": {
        "EAS_BUILD_PROFILE": "development"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "environment": "development"
    },
    "staging": {
      "distribution": "internal",
      "env": {
        "EAS_BUILD_PROFILE": "staging"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      }
    },
    "production": {
      "env": {
        "EAS_BUILD_PROFILE": "production"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      }
    }
  },
  "submit": {
    "preview": {
      "ios": {
        "appleId": "kouame@troodieapp.com",
        "ascAppId": "6746138280", 
        "appleTeamId": "M9RRL236WY"
      }
    },
    "development": {
      "ios": {
        "appleId": "kouame@troodieapp.com",
        "ascAppId": "6746138280", 
        "appleTeamId": "M9RRL236WY"
      }
    },
    "production": {
      "ios": {
        "appleId": "kouame@troodieapp.com",
        "ascAppId": "6746138280",
        "appleTeamId": "M9RRL236WY"
      },
      "android": {
        "serviceAccountKeyPath": "./path-to-your-service-account.json",
        "track": "production"
      }
    },
    "dev-testflight": {
      "ios": {
        "appleId": "kouame@troodieapp.com",
        "ascAppId": "6746138280",
        "appleTeamId": "M9RRL236WY"
      }
    }
  }
} 